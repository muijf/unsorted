{
  "name": "@muijf/utils",
  "version": "0.0.0",
  "description": "Contains unsorted useful functions and types that are not yet in their own package.",
  "keywords": [
    "utils"
  ],
  "homepage": "https://github.com/muijf/unsorted",
  "bugs": {
    "url": "https://github.com/muijf/unsorted/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muijf/unsorted.git",
    "directory": "packages/utils"
  },
  "license": "MIT",
  "author": "Muijf <contact@muijf.com>",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup"
  },
  "devDependencies": {
    "tsup": "^8.4.0"
  },
  "packageManager": "pnpm@8.6.7",
  "publishConfig": {
    "access": "public"
  }
}
